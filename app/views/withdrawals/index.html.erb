<p id="notice"><%= notice %></p>

<h1>Withdrawals</h1>

<table>
  <thead>
    <tr>
      <th>Integer</th>
      <th>Integer</th>
      <th>Integer</th>
      <th>Float</th>
      <th>String</th>
      <th>String</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @withdrawals.each do |withdrawal| %>
      <tr>
        <td><%= withdrawal.integer %></td>
        <td><%= withdrawal.integer %></td>
        <td><%= withdrawal.integer %></td>
        <td><%= withdrawal.float %></td>
        <td><%= withdrawal.string %></td>
        <td><%= withdrawal.string %></td>
        <td><%= withdrawal.date %></td>
        <td><%= link_to 'Show', withdrawal %></td>
        <td><%= link_to 'Edit', edit_withdrawal_path(withdrawal) %></td>
        <td><%= link_to 'Destroy', withdrawal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Withdrawal', new_withdrawal_path %>
